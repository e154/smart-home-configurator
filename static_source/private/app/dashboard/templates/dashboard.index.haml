.row
  .col-lg-12
    %h3.uptime.text-center
      %div(ng-if="total_uptime")
        {{total_uptime | uptime}}
      %div(ng-if="!total_uptime")
        &nbsp;

.row
  .col-lg-12
    .widgets-grid(gridster="gridsterOptions")
      %ul
        %li.widget-box(gridster-item="widget" ng-repeat="widget in dashboard.widgets")
          %div(dashboard-widget="widget")
